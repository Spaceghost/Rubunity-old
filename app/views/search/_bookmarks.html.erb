<% @results.each do |result| %>
<div style="border-bottom:1px #E8E8E8 solid; margin-bottom:15px; padding-bottom:15px">
  <p style="margin-bottom:5px"><strong><%= link_to result.title, bookmark_path(result) %></strong></p>
  <p>
    <%= result.excerpts.description.html_safe %><br />
  </p>
</div>
<% end %>
